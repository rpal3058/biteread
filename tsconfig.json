{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/tinymce/initFullProps.js",
    "components/tinymce/editor.ts",
    "components/editor/editor.js",
    "components/editor/contentEditor.tsxx",
    "components/editor/contentEditor.tsxx",
    "components/editor/markButton.js",
    "components/ui/toolbarComponents.js",
    "components/editor/customPlaceholder.js",
    "components/ui/rightHalf.js",
    "components/editor/blockButton.js"
  ],
  "exclude": ["node_modules"]
}
